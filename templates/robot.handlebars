{
  "name": "{{title}}",
  "defaultLocale": "en_US",
  "elements": [
    {{#list locators}}
    {
      "name": "{{{name}}}",
      "class": "{{{class}}}",
      "locale": [
        {
          "name": "en_US",
          "locator": "{{{locator}}}"
        }
      ],
      "wait": {
        "until": "visible",
        "for": 45
      }
    }
    {{/list}}
  ]
}
